dotspacemacs-configuration-layers
'(
     ansible
     auto-completion
     emacs-lisp
     git
     haskell
     helm
     html
     javascript
     markdown
     org
     osx
     (shell :variables
            shell-default-height 30
            shell-default-position 'bottom)
     spell-checking
     syntax-checking
     themes-megapack
     version-control
     yaml
     )

(setq-default dotspacemacs-default-font '("Menlo"
                                          :size 18
                                          :weight light
                                          :width expanded
                                          :powerline-scale 1.1))

(setq-default dotspacemacs-configuration-layers
              '(auto-completion
                (haskell :variables haskell-completion-backend 'intero)))

(setq-default dotspacemacs-themes 
                       '(
                         leuven
                         solarized-light
                         spacemacs-light
                         spacemacs-dark
                         solarized-dark
                         zenburn
                         monokai
                         ))
